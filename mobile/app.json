{
  "expo": {
    "name": "area",
    "slug": "area",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_app.png",
    "scheme": "area",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.muscularbird.area"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon_app.png",
        "backgroundImage": "./assets/images/icon_app.png",
        "monochromeImage": "./assets/images/icon_app.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.muscularbird.area",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon_app.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon_app.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you change your profile picture."
        }
      ],
      "expo-secure-store",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "EXPO_PUBLIC_IP": "10.18.207.161",
      "eas": {
        "projectId": "dc58b9ec-6bf9-410a-9428-ac56e1b95c4e"
      }
    },
    "owner": "etienne687"
  }
}
